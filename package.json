{
  "name": "pysearch",
  "main": "./lib/main",
  "version": "0.2.1",
  "description": "A better python documentation search",
  "keywords": [],
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "repository": "https://github.com/etotheiguy/pysearch",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "0.9.9",
    "axios": "^0.19.0"
  },
  "configSchema": {
    "python": {
      "title": "Python Executable",
      "order": 1,
      "type": "string",
      "default": "python3",
      "description": "Absolute path of your Python binary. This is used to launch the Python language server. Make sure to install `pyls` for this version of Python. Changes will take effect after a restart of the language server. Use `$PIPENV_PATH/bin/python` if you want to use the pipenv path of your project"
    },
    "fuzz": {
        "scope": "window",
        "type": "number",
        "enum": [
            0,
            1,
            2,
            3,
            4,
            5
        ],
        "default": "3",
        "description": "Search fuzziness with respect to current context. Can be changed in live query using the --fuzz (-f) flag."
    }
  },
  "consumedServices": {
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "enhancedScopes": [
    "source.python"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  }
}
